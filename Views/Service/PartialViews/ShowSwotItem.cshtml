@model Common.Dto.ServiceSwotDto
<table>

    @foreach (var property in ViewData.ModelMetadata.Properties)
    {
        if (property.PropertyName.Contains("Id") || property.PropertyName == "SwotActivities")
        {
            continue;
        }
        <tr>
            <td class="dt-cell">@(property.DisplayName ?? property.PropertyName)</td>
            <td class="dd-cell">@Html.Raw(property.Model)</td>
        </tr>
    }

    <tr>
        <td class="dt-cell">Activities</td>
        <td class="dd-cell">
            @if (Model.SwotActivities == null)
            {
                @Html.Action("ShowSwotActivities", Model.SwotActivities)
            }
            else
            {
                <label>no activities</label>
            }
            
        </td>
    </tr>



</table>
