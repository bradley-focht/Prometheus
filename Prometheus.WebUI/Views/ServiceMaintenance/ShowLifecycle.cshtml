@model Common.Dto.LifecycleStatusDto
@{
    ViewBag.Title = "Lifecycle";
    ViewBag.Links = new List<KeyValuePair<string, string>> { new KeyValuePair<string, string>("Home", "Home"),
                                                             new KeyValuePair<string, string>("Services Maintenance", "ServiceMaintenance/Index"),
                                                             new KeyValuePair<string, string>(ViewBag.Title, "#")};   
    Layout = "~/Views/Shared/_SidebarLayout.cshtml";
}

@section Sidebar {
    @Html.Action("ShowLifeCycleList", Model.Id)
}


@section Content {
   @if (Model.Id != 0)
   {
       @Html.Partial("/Views/ServiceMaintenance/PartialViews/LifecycleViewer.cshtml", Model);
   }
}
