@using Prometheus.WebUI.Models.Shared
@model Prometheus.WebUI.Models.ServiceMaintenance.LifecycleModel
@{
    ViewBag.Title = "Delete " + Model.CurrentStatus.Name;
    ViewBag.Links = new List<KeyValuePair<string, string>> { new KeyValuePair<string, string>("Home", "Home"),
                                                             new KeyValuePair<string, string>("Services Maintenance", "ServiceMaintenance/Index"),
                                                             new KeyValuePair<string, string>("Lifecycle", "ServiceMaintenance/ShowLifecycle"),

                                                                new KeyValuePair<string, string>(ViewBag.Title, "#")};


    Layout = "~/Views/Shared/_SidebarLayout.cshtml";

    LinkListModel linkListModel = new LinkListModel(Model.Statuses, Model.CurrentStatus.Id, "ShowLifecycle", "AddLifecycle", "Lifecycle Statuses");
    ConfirmDeleteModel deleteModel = new ConfirmDeleteModel(Model.CurrentStatus.Id, Model.CurrentStatus.Name, "DeleteLifecycle", "ShowLifeCycle", "ServiceMaintenance");
}


@section Sidebar
{
    @Html.Partial("/Views/Shared/PartialViews/_LinkList.cshtml", linkListModel)
}

@section Content
{
    @Html.Partial("/Views/Shared/PartialViews/_ConfirmDelete.cshtml", deleteModel)
}
