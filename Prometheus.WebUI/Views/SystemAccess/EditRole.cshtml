@model Prometheus.WebUI.Models.SystemAccess.RoleModel

@{
    ViewBag.Title = "System Access";
    ViewBag.SubTitle = "Add Role";
    Layout = "~/Views/Shared/_SidebarLayout.cshtml";

    ViewBag.Links = new List<KeyValuePair<string, string>>
    {
        new KeyValuePair<string, string>("Home", "Home"),
        new KeyValuePair<string, string>(ViewBag.Title, "SystemAccess/Index"),
        new KeyValuePair<string, string>(ViewBag.SubTitle, "#")
    };
}


@section Sidebar
{
	@Html.Action("ShowRoles", new { Id = 0})
}

@section Content
{
	<div class="title-div">@ViewBag.SubTitle</div>

	@Html.Partial("PartialViews/RolesEditor", Model.Role)

}
