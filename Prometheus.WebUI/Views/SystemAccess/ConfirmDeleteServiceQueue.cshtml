@model Common.Dto.ServiceQueueDto
@{
	ViewBag.Title = "System Access";
	ViewBag.SubTitle = "Confirm Delete Queue";
	Layout = "~/Views/Shared/_Layout.cshtml";

	ViewBag.Links = new List<KeyValuePair<string, string>>
	{
		new KeyValuePair<string, string>("Home", "Home"),
		new KeyValuePair<string, string>("System Access", "SystemAccess/Index"),
		new KeyValuePair<string, string>(ViewBag.SubTitle, "#"),
	};
}

<div class="title-div">@ViewBag.SubTitle</div>

@using (Html.BeginForm("DeleteQueue", "SystemAccess", FormMethod.Post))
{
	<div class="panel-div">
		<div class="present-section">
			
		</div>
	</div>

	<div class="flex-container-col">
		<div class="panel-footer">
			<div class="float-right-div">
				@Html.ActionLink("Back", "ShowQueues", null, new {@class = "btn btn-back btn-default"})
				<input type="submit" value="Confirm Delete" class="btn btn-danger"/>
			</div>
		</div>
	</div>

}