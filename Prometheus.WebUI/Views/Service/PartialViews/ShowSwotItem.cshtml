@model Common.Dto.ServiceSwotDto
<table>

    @foreach (var property in ViewData.ModelMetadata.Properties)
    {
        if (property.PropertyName.Contains("Id"))
        {
            continue;
        }
        <tr>
            <td class="dt-cell">@(property.DisplayName ?? property.PropertyName)</td>
            <td class="dd-cell">@Html.Raw(property.Model)</td>
        </tr>
    }



</table>
