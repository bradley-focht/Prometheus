@{
    ViewBag.Title = "Home";
    ViewBag.Links = null;
    Layout = "~/Views/Shared/_BasicLayout.cshtml";
}
@Html.Partial("PartialViews/_TitleBar", null)
<div class="panel panel-primary" style="margin-top: -15px;">
    <div class="panel-heading">
        <h1>Prometheus IT Service Management</h1>
    </div>
    <div class="panel-body" style="background-color: #eef2f7;">
        <table>
            <tr>
                <td style="width: 50%" class="content-cell">
                    <a href="@Url.Action("Index", "ServicePortfolio")">
                        <img src="~/Content/Images/invest.png" alt="Service Portfolio" style="height: 72px; width: 77px;">
                    </a>
                </td>
                <td style="width: 50%;" class="content-cell">
                    <a href="@Url.Action("Index", "Service")">
                        <img src="~/Content/Images/pie.png" alt="Services" style="height: 68px; width: 69px;">
                    </a>
                </td>
            </tr>
            <tr>
                <td class="content-cell">

                    @Html.ActionLink("Service Portfolio", "Index", "ServicePortfolio", null, new { @class = "font-home-link" })
                </td>
                <td class="content-cell">@Html.ActionLink("Services", "Index", "Service", null, new { @class = "font-home-link" })</td>

            </tr>
            <tr>
                <td>
                    <div style="height: 10px;"></div>
                </td>
            </tr>
            <tr>
                <td class="content-cell"><img src="~/Content/Images/presentation.png" alt="Services" style="height: 72px; width: 77px;"></td>
                <td class="content-cell"><img src="~/Content/Images/presentation.png" alt="Services" style="height: 72px; width: 77px;"></td>
            </tr>
            <tr>
                <td class="content-cell"><label class="font-home-link">@Html.ActionLink("Business Service Catalog", "BusinessCatalog", "Catalog", null, new { @class = "font-home-link" })</label></td>
                <td class="content-cell">@Html.ActionLink("Supporting Service Catalog", "SupportCatalog", "Catalog", null, new { @class = "font-home-link" })</td>

            </tr>
            <tr>
                <td>
                    <div style="height: 10px;"></div>
                </td>
            </tr>
            <tr>
                <td class="content-cell"><img src="~/Content/Images/people-network.png" alt="Services" style="height: 82px; width: 74px;"></td>
                <td class="content-cell"><img src="~/Content/Images/shake.png" alt="Services" style="height: 70px; width: 88px;"></td>
            </tr>
            <tr>
                <td class="content-cell"><label class="font-home-link">Submit a Service Request</label></td>
                <td class="content-cell"><label class="font-home-link">Service Request Approvals</label></td>
            </tr>
        </table>
    </div>
    <div class="panel-footer" style="padding-top: 10px; margin-top: -10px;">
        <table style="width: 100%">
            <tr>
                <td></td>
            </tr>
            <tr>
                <td style="width: 33%">
                    @Html.ActionLink("System Access", "Index", "SystemAccess")
                <br />@Html.ActionLink("Services Maintenance", "Index", "ServiceMaintenance")
            </td>
            <td style="width: 33%; vertical-align: top;">Service request templates</td>
            <td></td>
                <td style="width: 33%">Scripts <br/>Fullfilment Queue</td>
        </tr>
    </table>
</div>
    <div class="panel-footer">
        <span class="copyright-notice">&copy;2017 Prometheus</span>
    </div>
</div>