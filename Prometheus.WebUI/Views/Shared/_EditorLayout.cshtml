@using Prometheus.WebUI.HtmlHelpers;

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Prometheus</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PrometheusCustom.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">

        <label class="navbar-brand">@ViewBag.Title</label>
    </div>
    <div class="breadcrumb-container">
        @RenderSection("Breadcrumb")
    </div>

    <div class="std-indent">
        <table class="std-table" style="width:100%">
            <tr>
                <td></td>
                <td>@RenderBody()</td>

            </tr>
            <tr>
                <td colspan="2"></td>
            </tr>

        </table>
    </div>
</body>
</html>