@model Prometheus.WebUI.Models.DeleteConfirmationModel

<div class="std-div">
    <div>
        Are you sure you want to delete this bitch?
    </div>
    <div class="panel panel-footer" style="width:100%;">
        <div class="float-right-div">
            @Html.ActionLink("Cancel", Model.CancelAction, Model.CancelArgs, new { @class = "btn btn-default" })
            @Html.ActionLink("Confirm", Model.DeleteAction, new { id = 0 }, new { @class = "btn btn-danger" })
        </div>
    </div>
</div>


