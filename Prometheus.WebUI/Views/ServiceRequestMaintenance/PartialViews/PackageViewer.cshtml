
@model Prometheus.WebUI.Models.ServiceRequestMaintenance.PackageViewModel

@if (Model != null)
{
    <div class="dd">
	    <div class="dd-row">
		    <div class="dt-cell">Name</div>
		    <div class="dd-cell">@Model.ServiceRequestPackage.Name</div>
	    </div>
		<div class="dd-row">
			<div class="dt-cell">Action</div>
			<div class="dd-cell">@Model.ServiceRequestPackage.Action</div>
		</div>
        <div class="dd-row">
            <div class="dt-cell">
                Associations
            </div>
            <div class="dd-cell">
                @foreach (var item in Model.PackageItems)
                {
                    <div>@item.Name</div>
                }
            </div>
        </div>
        
    </div>
}

